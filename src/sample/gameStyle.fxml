<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0"
      prefWidth="300.0" spacing="50.0"
      fx:controller="sample.GameStyleController">
    <Button fx:id="hLocal" mnemonicParsing="false" text="Human Local" />
    <Button fx:id="hOnline" mnemonicParsing="false" text="Human Online" />
    <Button fx:id="computer" mnemonicParsing="false" text="Computer" />
    <padding>
        <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
    </padding>
</VBox>